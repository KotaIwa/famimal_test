<section class="listings">
  <div class="wrapper">
    <ul class="properties_list">
      <% @pets.each do |pet| %>
      <li>
        <a href="pets/<%= pet.id %>">
          <img src="<%= pet.image_url %>" alt="" title="" class="property_img"/>
        </a>
        <span class="price"><%= pet.price %>円</span>
        <div class="property_details">
          <h1>
            <a href="#"><%= pet.breed %></a>
          </h1>
          <h2>生年月日:</span></h2>
        </div>
      </li>
      <% end %>
    </ul>
  </div>
</section>  <!--  end listing section  -->